{
  "name": "@are-visual/icon",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm svgr && pnpm lint",
    "lint": "eslint \"src/**/*.{ts,tsx,js}\" --fix",
    "svgr": "npx @svgr/cli --out-dir src --index-template ./template/default-index.js --jsx-runtime automatic --no-dimensions --typescript -- assets"
  },
  "buildOptions": {
    "entry": "./src/index.ts"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1"
  }
}